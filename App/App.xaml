<Application
    x:Class="App.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:App"
    xmlns:mvvmlight="using:GalaSoft.MvvmLight.Extensions.Xaml"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <local:Locator x:Key="Locator" />

            <mvvmlight:ViewModelConverterPipe x:Key="viewModelConverter">
                <mvvmlight:ViewModelConverterPipe.Map>
                    <mvvmlight:Pair View="local:Blank" ViewModel="local:BlankViewModel"/>
                </mvvmlight:ViewModelConverterPipe.Map>
            </mvvmlight:ViewModelConverterPipe>

        </ResourceDictionary>
    </Application.Resources>
</Application>
